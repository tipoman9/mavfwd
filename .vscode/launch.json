{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "C Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mavfwd",
            "args": [                
                "--master",
                "/dev/ttyUSB2",
                "--baudrate",
                "115200",
                "--out",
                "127.0.0.1:14550",                        
                "--channel",
                "7",
                "-a",
                "5",
                "--osd",
                " ",
                "-f",                
                "/home/home/",
                "--wfb",               
                " "

            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            
            "setupCommands": [
                {
                    "description": "MAVLINk gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc build",
            "linux": {
                "preLaunchTask": "gcc build"
            }
            
        }
    ]
}
